    <span class="s2">"concept"</span>: <span class="s2">"7737c9ef-80ad-44e6-920f-4106da899df4"</span>,
    <span class="s2">"initial"</span>: <span class="nb">true</span>,
    <span class="s2">"terminal"</span>: <span class="nb">false</span>
